{% extends 'base.html.twig' %}

{% block title %}Available Exams{% endblock %}
{% block javascripts %}
  {% block importmap %}{{ importmap('student') }}{% endblock %}
{% endblock %}
{% block stylesheets %}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
{% endblock %}
{% block body %}
<style>
    /* .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; } */
</style>
<div class="container exam-result-container">
  <div class="exams-available">
  <a href="/logout" class="btn btn-danger float-end">Logout</a><br>
    <table class="table table-striped table-bordered">
    <thead>
      <td>Exam Number</td>
      <td>Marks</td>
    </thead>
    {% for result in results %}
      <tr>
        <td>{{result.ExamNumber}}</td>
        <td>{{result.Marks}}</td>
      </tr>
    {% endfor %}
  </div>
  
  
  </table>
  <span><a href="/student" class="btn btn-primary float-end dashboard-button">Go back to dashboard</a></span>
</div>
{% endblock %}
